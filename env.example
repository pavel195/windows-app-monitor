# Токен Telegram бота (обязательный)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# ID пользователей Telegram, которым разрешен доступ (через запятую)
# Например: TELEGRAM_ALLOWED_USERS=123456789,987654321
# Оставьте пустым, чтобы разрешить доступ всем
TELEGRAM_ALLOWED_USERS=

# ID чата для отправки уведомлений (группа, канал или пользователь)
# Если не указано, уведомления будут отправляться пользователям из TELEGRAM_ALLOWED_USERS
# TELEGRAM_CHAT_ID=-1001234567890
TELEGRAM_CHAT_ID=

# Имя журнала Windows для мониторинга
EVENT_LOG_NAME=Application

# Интервал проверки журнала (в секундах)
CHECK_INTERVAL=5

# Список приложений для отслеживания (через запятую)
# Например: TRACKED_APPS=chrome.exe,firefox.exe
# Оставьте пустым, чтобы отслеживать все приложения
TRACKED_APPS=

# URL видео для открытия через бота
VIDEO_URL=https://www.youtube.com/watch?v=xm3YgoEiEDc&ab_channel=10Hours 